<Brushes>

  <Brush Name="Launcher.Mods.IssueText" Font="FiraSansExtraCondensed-Regular" TextHorizontalAlignment="Left" TextVerticalAlignment="Center">
    <Layers>
    </Layers>
    <Styles>
      <Style Name="Default" FontColor="#E6C8A6FF" TextAlphaFactor="1.0" TextBlur="0" TextOutlineAmount="0" TextGlowRadius="0" FontSize="18" />
    </Styles>
  </Brush>

  <Brush Name="Launcher.Mods.ExpandIndicator">
    <Layers>
      <BrushLayer Name="Default" />
    </Layers>
    <Styles>
      <Style Name="Default">
        <BrushLayer Name="Default" Sprite="arrow_down" Color="#E6C8A6FF" />
      </Style>
      <Style Name="Selected">
        <BrushLayer Name="Default" Sprite="arrow_left" Color="#E6C8A6FF" />
      </Style>
    </Styles>
  </Brush>

</Brushes>